{
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "no-console": 0,
        "no-cond-assign": 0,
        "no-unused-vars": 1,
        "no-extra-semi": "warn",
        "semi": "warn",
        "prettier/prettier": "error"
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ]
}